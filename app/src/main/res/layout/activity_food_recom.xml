<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FoodRecomActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <!-- Vector -->
        <View
            android:id="@+id/vector"
            android:layout_width="16dp"
            android:layout_height="14dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/backpink_vektor"
            />

        <!-- Group 140 -->
        <androidx.cardview.widget.CardView
            android:id="@+id/breakfastt"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/group_140"
                android:layout_width="328dp"
                android:layout_height="209dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="16dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="16dp"
                android:src="@drawable/foodrec_vek"
                />

            <View
                android:id="@+id/rectangle_8"
                android:layout_width="match_parent"
                android:layout_height="160dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="66dp"
                android:background="@drawable/foodrecomendation_vektor" />
            <View
                android:id="@+id/rectangle2_8"
                android:layout_width="match_parent"
                android:layout_height="160dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="66dp"
                android:background="@drawable/foodrecomendation_vektor" />

            <!-- Breakfast -->
            <TextView
                android:id="@+id/breakfast"
                android:layout_width="wrap_content"
                android:layout_height="38dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="32dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="159dp"
                android:text="Breakfast"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/white"
                android:textSize="25dp"
                android:gravity="top"
                />

            <!-- 07:00 - 08.00 -->
            <TextView
                android:id="@+id/some_id"
                android:layout_width="118dp"
                android:layout_height="26dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="33dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="190dp"
                android:text="07:00 - 08:00"
                android:textSize="17dp"
                android:fontFamily="@font/poppins"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:gravity="top"
                />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:id="@+id/group_141"
                android:layout_width="328dp"
                android:layout_height="209dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="19dp"
                android:layout_marginLeft="16dp"
                android:src="@drawable/lunch_vektor" />

            <View
                android:id="@+id/rectangle_9"
                android:layout_width="match_parent"
                android:layout_height="160dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="67dp"
                android:background="@drawable/foodrecomendation_vektor" />

            <!-- Lunch -->
            <View
                android:id="@+id/rectangle_91"
                android:layout_width="match_parent"
                android:layout_height="160dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="67dp"
                android:background="@drawable/foodrecomendation_vektor" />

            <TextView
                android:id="@+id/Lunch"
                android:layout_width="120dp"
                android:layout_height="38dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="32dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="159dp"
                android:text="Lunch"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/white"
                android:textSize="25dp"
                android:gravity="top"
                />

            <!-- 13:00 - 14.00 -->
            <TextView
                android:id="@+id/jamSiang"
                android:layout_width="118dp"
                android:layout_height="26dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="33dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="190dp"
                android:text="13:00 - 14.00"
                android:textSize="17dp"
                android:fontFamily="@font/poppins"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:gravity="top"
                />
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:id="@+id/group_142"
                android:layout_width="328dp"
                android:layout_height="209dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="19dp"
                android:src="@drawable/dinner_vektor"/>

            <View
                android:id="@+id/rectangle_10"
                android:layout_width="328dp"
                android:layout_height="160dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="67dp"
                android:background="@drawable/foodrecomendation_vektor" />
            <View
                android:id="@+id/rectangle_101"
                android:layout_width="328dp"
                android:layout_height="160dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="67dp"
                android:background="@drawable/foodrecomendation_vektor" />

            <!-- Dinner -->
            <TextView
                android:id="@+id/Dinner"
                android:layout_width="120dp"
                android:layout_height="38dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="32dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="159dp"
                android:text="Dinner"
                android:fontFamily="@font/poppins_medium"
                android:textColor="@color/white"
                android:textSize="25dp"
                android:gravity="top"
                />

            <!-- 18:00 - 19.00 -->
            <TextView
                android:id="@+id/jamMalam"
                android:layout_width="118dp"
                android:layout_height="26dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="33dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="190dp"
                android:fontFamily="@font/poppins"
                android:text="18:00 - 19.00"
                android:textSize="17dp"
                android:textColor="@color/white"
                android:textStyle="bold"
                android:gravity="top"
                />
        </androidx.cardview.widget.CardView>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>